.breadcrumbs
  .container
    ul.breadcrumbs__list
       - var items = ["Главная", "Каталог"]
            ul.nav__list
                each item in items
                    li(class="breadcrumbs__item  breadcrumbs__item"+ ((item == "Каталог") ? "_active" : ""))
                        a(href="#").breadcrumbs__link= item